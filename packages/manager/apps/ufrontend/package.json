{
  "name": "@ovh-ux/ufrontend",
  "version": "0.0.0",
  "private": true,
  "description": "OVHcloud micro-frontend.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ovh/manager.git",
    "directory": "packages/manager/apps/ufrontend"
  },
  "license": "BSD-3-Clause",
  "author": "OVH SAS",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm-run-all build:navbar copy:navbar build:template",
    "build:navbar": "lerna run --scope '@ovh-ux/fragment-navbar' build",
    "build:template": "mkdir -p dist/template/ && cp src/template/*.html dist/template/",
    "copy:navbar": "mkdir -p dist/navbar/ && cp ../../modules/fragment-navbar/dist/index.js dist/navbar/index.js",
    "dev": "http-server dist -p 9001"
  },
  "dependencies": {
    "@ovh-ux/ovh-ufrontend": "^0.0.0"
  },
  "devDependencies": {
    "@ovh-ux/component-rollup-config": "^7.0.0",
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5"
  }
}
